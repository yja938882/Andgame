<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="game.juan.andenginegame0.ygamelibs.Test.StartTestActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <Button
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:text="unit test"
            android:onClick="onClickUnitTest"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <EditText
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:inputType="number"
                android:id="@+id/build_map_test"
                android:text="0"/>
            <Button
                android:layout_width="200dp"
                android:layout_height="50dp"
                android:text="map build test"
                android:onClick="onClickBuildMapTest"/>

        </LinearLayout>

        <Button
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:text="obstacle test"
            android:onClick="onClickObstacleTest"/>
        <Button
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:text="local json load"
            android:onClick="onClickLoadDataTest"/>




    </LinearLayout>
</android.support.constraint.ConstraintLayout>
